SELECT A.cd_laudo,
A.CD_ATENDIMENTO,
A.DT_EMISSAO,
A.CD_PRESTADOR_SOL,
(SELECT B.NM_PRESTADOR FROM DBAMV.PRESTADOR B WHERE A.CD_PRESTADOR_SOL = B.CD_PRESTADOR) AS NM_PRESTADOR_SOL,
A.CD_PROCEDIMENTO,
A.CD_ESPEC_SUS,
A.CD_PRESTADOR,
B.NM_PRESTADOR,
A.CD_CID_PRINCIPAL,
D.NM_PACIENTE

FROM dbamv.LAUDO_AIH A,
DBAMV.PRESTADOR B,
DBAMV.ATENDIME C,
DBAMV.PACIENTE D

WHERE DT_EMISSAO between  to_date('30/08/2012', 'dd/mm/yyyy') and to_date('31/08/2012', 'dd/mm/yyyy')
AND  (A.CD_PRESTADOR= B.CD_PRESTADOR)
AND A.CD_ATENDIMENTO = C.CD_ATENDIMENTO
AND C.CD_PACIENTE = D.CD_PACIENTE
